{% extends 'partials/base.html.twig' %}

{% block content %}

<div id="content">
    <div class="flex">

      <div class="column">

        {% for image in page.media.files %}
            {{ image.html }}
        {% endfor %}
      </div>

      <div class="column">
        <h1>{{ page.title }}</h1>
          {{ page.content }}

      </div>

    </div>
</div>

{% endblock %}
