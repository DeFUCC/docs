{% extends 'partials/base.html.twig' %}

{% block content %}

<div id="body-inner" class="markdown-body">
    <div class="row">
        
      <div class="col col-4">
         
        {% for image in page.media.files %}
            {{ image.html }}
        {% endfor %}
      </div>
      
      <div class="col">
        <h1>{{ page.title }}</h1>
          {{ page.content }}
        
      </div>

    </div>
</div>

{% endblock %}
